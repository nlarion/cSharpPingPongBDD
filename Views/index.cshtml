<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Enter a Number</title>
  </head>
  <body>
    <h1>Enter a Number:</h1>
    <form action="/" method="post">
      <input type="text" name="userNumber">
      <button type="submit" name="button">hit submit</button>
    </form>
    <ul>
      @{
        if(Model != null)
        {
          foreach(var pingping in Model)
          {
            <li>@pingping</li>
          }
        }
      }
    </ul>
  </body>
</html>
